taveC_ON=$( head -1 $file )
if [[ -f "$file" && ( "$taveC_ON" = "0" || "$taveC_ON" = "1" ) ]]; then
echo "OK"
else
echo "0" > $file
taveC_ON=0
fi
if [ "${taveC_ON}" = "0" ];
then
menuitems+=( "taveC1" "START temporal average of DM correction -> aol2_dmCave" )
else
menuitems+=( "taveC0" "STOP temporal average of DM correction  -> aol2_cmCave" )
fi


menuitems+=( " " " " )

