echo "0" > $file
wfrefO=$( cat conf/conf_dmwrefO_name.txt )
tmuxname="GPUdm2wfsrefM_dm${LOOPNUMBER}"
tmux send-keys -t $tmuxname "" C-c
sleep 0.1
tmux send-keys -t $tmuxname "exit" C-m
sleep 0.1
