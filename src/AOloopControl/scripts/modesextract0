#! /bin/bash


loopnb=$(cat LOOPNUMBER)

pname="aol${loopnb}modeval0"


Cfits -n $pname << EOF
csetpmove aolRT1
loadfits "conf/aol${loopnb}_DMmodes.fits" DMmodes
readshmim aol${loopnb}_meas_act
creaim aol${loopnb}_gainarray 50 1
readshmim aol${loopnb}_dmmask
cudaextrmodes aol${loopnb}_meas_act DMmodes aol${loopnb}_gainarray aol${loopnb}_modeval 5 0 empty empty aol${loopnb}_dmmask
exit
EOF
